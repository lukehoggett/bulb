{
  "name": "Bulb",
  "version": "0.0.3",
  "description": "PLaybulb led control panel",
  "main": "main/index.js",
  "scripts": {
    "start": "gulp && sudo ./node_modules/electron/dist/electron ./main/dist/index.js --displaysize=sm | node_modules/bunyan/bin/bunyan -o long",
    "sm": "gulp && sudo ./node_modules/electron/dist/electron ./main/dist/index.js --displaysize=sm | node_modules/bunyan/bin/bunyan -o long",
    "md": "gulp && sudo ./node_modules/electron/dist/electron ./main/dist/index.js --displaysize=md | node_modules/bunyan/bin/bunyan -o long",
    "lg": "gulp && sudo ./node_modules/electron/dist/electron ./main/dist/index.js --displaysize=lg | node_modules/bunyan/bin/bunyan -o long",
    "full": "gulp && sudo ./node_modules/electron/dist/electron ./main/dist/index.js --displaysize=full | node_modules/bunyan/bin/bunyan -o long",
    "rpi": "sudo ./node_modules/electron/dist/electron ./main/dist/index.js --displaysize=full | bunyan -o short",
    "postinstall": "./postinstall.sh"
  },
  "keywords": [
    "electron-app"
  ],
  "author": "Luke Hoggett",
  "repository": "https://github.com/lukehoggett/bulb",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-electron": "^0.37.8",
    "del": "^2.2.2",
    "devtron": "^1.4.0",
    "electron": "^1.4.4",
    "electron-connect": "^0.6.0",
    "electron-rebuild": "^1.3.0",
    "eslint": "^3.9.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.4.1",
    "gulp": "^3.9.1",
    "gulp-atom-electron": "^1.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^3.0.0",
    "gulp-eslint": "^3.0.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-run-electron": "^3.0.1",
    "gulp-sass": "^3.0.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "jspm": "^0.16.47",
    "jspm-bower-endpoint": "^0.3.2",
    "merge-stream": "^1.0.0",
    "run-sequence": "^1.2.2"
  },
  "jspm": {
    "directories": {
      "baseURL": "renderer"
    },
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.5.8",
      "angular-animate": "github:angular/bower-angular-animate@^1.5.8",
      "angular-localforage": "npm:angular-localforage@^1.3.2",
      "angular-material": "github:angular/bower-material@^1.1.1",
      "angular-material-icons": "npm:angular-material-icons@^0.7.0",
      "angular-order-object-by": "bower:angular-order-object-by@^1.3.0",
      "angular-route": "github:angular/bower-angular-route@^1.4.7",
      "angular-ui-router": "github:angular-ui/angular-ui-router-bower@^0.3.1",
      "brianpkelley/md-color-picker": "github:brianpkelley/md-color-picker@develop",
      "defunctzombie/node-uuid": "github:kelektiv/node-uuid@^3.0.1",
      "md-color-picker": "github:brianpkelley/md-color-picker@develop",
      "mdi": "npm:mdi@^1.7.22",
      "mozilla/localForage": "github:localForage/localForage@^1.4.2",
      "ocombe/angular-localForage": "github:ocombe/angular-localForage@^1.3.1",
      "tinycolor2": "npm:tinycolor2@^1.4.1"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "dependencies": {
    "async": "^2.1.2",
    "bower": "^1.7.9",
    "bunyan": "^1.8.4",
    "config": "^1.21.0",
    "immutable": "^3.8.1",
    "json5": "^0.5.0",
    "jspm-bower-endpoint": "^0.3.2",
    "noble": "^1.7.0",
    "node-persist": "^2.0.7",
    "yargs": "^6.3.0"
  }
}
